{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "esModuleInterop": true,
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": true,
    "jsx": "react",
    "lib": ["dom", "es2017"]
  }
}
